{% extends 'MrokGraphBundle::layout.html.twig' %}


{% block content %}
<div class="row">
    <div class="span4">
        <div class="well" id="left-column">
            {% block left_column %}
            {% endblock %}
        </div>
    </div>
    <div class="span8">
        <div class="well">
            {% block right_column %}
            <img src="{{ app.request.getBasePath()}}/bundles/mrokgraph/images/ajax-loader.gif" id="result-loader"
                 class="hide"/>

            <div id="result-block" class="hide">
                <h4>{{pageHeader}}</h4>
                <ul></ul>
            </div>
            <br/>
            <img src="{{ app.request.getBasePath()}}/bundles/mrokgraph/images/graph.png">
            {% endblock %}
        </div>
    </div>
</div>
{% endblock %}
